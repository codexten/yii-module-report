{
  "name": "codexten/yii-module-report",
  "type": "library",
  "description": "report module for yii 2.0 or higher",
  "require": {
    "codexten/yii-web": "~2.0.0",
    "codexten/yii-dataview": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\report\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "core": "config/core.php",
      "moduleReport": "config/moduleReport.php"
    }
  }
}
